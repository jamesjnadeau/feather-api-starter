{
  "name": "gigofund",
  "version": "1.0.0",
  "description": "A website deditcate to raising funds for a Veteran servicing non-profit organization.",
  "private": true,
  "scripts": {
    "start": "npm run build && pm2 start pm2-production.json",
    "build": "node_modules/.bin/webpack --progress --colors -d -p",
    "watch": "node_modules/.bin/webpack --progress --colors -d --watch",
    "pm2": "pm2",
    "allstop": "pm2 delete all",
    "api": "pm2 start ./api/bin/www",
    "api:stop": "pm2 stop ./api/bin/www",
    "frontend:watch": "pm2 start ./frontend/dev-sever.js",
    "dev": "pm2 start pm2-development.json && npm run dev:logs",
    "dev:logs": "pm2 logs",
    "dev:stop": "pm2 delete pm2-development.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "James J Nadeau",
  "dependencies": {
    "ContentTools": "git+https://github.com/GetmeUK/ContentTools.git",
    "async": "^1.5.0",
    "body-parser": "~1.13.2",
    "bootable": "^0.2.4",
    "bootstrap": "^3.3.6",
    "bootstrap-notify": "^3.1.3",
    "connect-flash": "^0.1.1",
    "connect-redis": "^3.0.1",
    "cookie-parser": "~1.3.5",
    "corser": "^2.0.0",
    "css-loader": "^0.23.0",
    "debug": "~2.2.0",
    "dotenv": "^1.2.0",
    "express": "~4.13.1",
    "express-session": "^1.12.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "feathers": "^1.2.1",
    "feathers-client": "^0.4.0",
    "feathers-hooks": "^0.5.1",
    "feathers-mongoose": "^2.0.0",
    "file-loader": "^0.8.5",
    "image-webpack-loader": "^1.6.2",
    "jade": "^1.11.0",
    "jquery": "^2.1.4",
    "jquery-ui": "^1.10.5",
    "jsoneditor": "^4.2.1",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "loader-utils": "^0.2.12",
    "lodash": "^3.10.1",
    "log4js": "^0.6.29",
    "mongoose": "^4.3.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^3.1.0",
    "pm2": "^0.15.10",
    "require-directory": "^2.1.1",
    "serve-favicon": "~2.3.0",
    "static-site-loader": "0.0.3",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.14.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "devDependencies": {
    "open": "0.0.5"
  }
}
